dim i,px,py:integer
dim x,y,xz,yz,xt:float
for px=0 to 21
for py=0 to 31
xz=px*3.5/32.-2.5
yz=py*2./22.-1.
x=0.
y=0.
for i=0 to 14
if x*x+y*y>4 then goto 230
xt=x*x-y*y+xz
y=2*x*y+yz
x=xt
next i
230 rem plot
run gfx2("CURXY",px,py)
run gfx2("COLOR",1,i)
print " ";
next px
next py
call gfx2("COLOR",1,0)
