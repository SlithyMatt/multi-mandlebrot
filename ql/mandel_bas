100 im=0
110 WINDOW 512,230,0,0:PAPER 0:CLS
120 CSIZE 1,0
130 t=DATE
140 FOR py=0 TO 21
150 FOR px=0 TO 31
160 xz=px*3.5/32-2.5
170 yz=py*2/22-1
180 x=0:y=0
190 FOR i=0 TO 14
200 IF x*x+y*y>4 THEN GO TO 270
210 xt=x*x-y*y+xz
220 y=2*x*y+yz
230 x=xt
240 NEXT i
250 i=15
260 i=15-i
270 IF i MOD 2==0 THEN PAPER INT(i/2):ELSE PAPER INT(i/2),INT(i/2)+1
280 PRINT " ";
290 IF i>im THEN im=i
300 NEXT px
310 PRINT
320 NEXT py
330 PRINT #0,DATE-t
340 STOP
